/* pages/vipcard/vipcard.wxss */
page{
    min-height: 100%;
    background-color: #F2f2f2;
  }

  view {
    font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
  }

  .container {
    min-height: 100%;
    width: 100%;
  }

  .title-view{
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      height: 70rpx;
      background-color: #fff;
      line-height: 70rpx;
      padding-left: 20rpx;
      padding-right: 20rpx;
  }

  .title-view .myvip{
      font-size: 24rpx;
      color: #333333;
  }

  .title-view .sale-detail{
      font-size: 24rpx;
      color: #0096ec;
  }

  .card-view{
      height: 292rpx;
      background-color: #fff;
      margin: 18rpx 12rpx 0rpx 12rpx;
      border-radius: 16rpx;
  }

  .card-view .card-body{
    background-image: url("data:image/svg+xml;base64,");
    background-size: cover;
    display: inline-block;
    width: 152rpx;
    height: 152rpx;
  }
  
  @font-face {
    font-family: 'iconfont';
    src: url('../../images/svg-file/iconfont.eot');
    src: url('../../images/svg-file/iconfont.eot?#iefix') format('embedded-opentype'),
    url('../../images/svg-file/iconfont.woff') format('woff'),
    url('../../images/svg-file/iconfont.ttf') format('truetype'),
    url('../../images/svg-file/iconfont.svg#iconfont') format('svg');
  }

  .iconfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
  }