.car-text-view {
  width: 390rpx;
  display: inline-block;
  /*display: flex;
  flex-direction: column;*/
  text-align: left
}

.main-text-view {
  margin-top: 44rpx;
  font-size: 34rpx;
  color: #333333;
  font-weight: bold;
  white-space: pre-wrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical

}
.no-main-text-view {
  margin-top: 44rpx;
  font-size: 34rpx;
  color: #FE7572;
  font-weight: bold;
}
.text-view {
  font-size: 22rpx;
