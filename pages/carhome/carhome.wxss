.page-body {
  display: flex;
  flex-direction: column;
  flex-grow: 1;
  overflow-x: hidden;
  align-items: center;
  width: 100%;
  height: 100%;
  font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif
}

.body-view{
  width: 100%;
  height: 100%;
  font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif
}

.ad-view {
  width: 100%;
  height: 470rpx;
  background: #fff
}

.slide-image {
  width: 100%;
  height: 100%
}

.slide-image .wx-swiper-dot {
  margin-bottom: 10rpx
}

.search {
  width: 718rpx;
  background: #fff;
  position: absolute; 
  top: 20rpx; 
  z-index: 1;
  left: 16rpx;
  border-radius: 1000rpx;
  height: 62rpx;
  text-align: center; 
  display: flex;
  flex-direction: row-reverse
}

.searchcion {  
  width: 28rpx;
  position: absolute;
  z-index: 2;
  top: 17rpx;
  left: 245rpx
}

.searchinput {  
  width: 666rpx;
  height: 100%
}

.searchinput-select {  
  width: 100%;
  height: 100%
}

.place-search-intput {  
  font-size: 28rpx;
  color: #b7b7b7
}

.select-car {
  width: 700rpx;
  height: 160rpx;
  position: absolute;
  top: 448rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  border-radius: 8rpx;
  box-shadow: 0rpx 0rpx 9rpx rgba(0, 0, 0, 0.07);
  /*box-shadow: (4,0,0),4px 11px (90â€™) 7%;*/
  text-align: center; 
  background: #fff;
  display: flex;
  flex-direction: row
}

.car-brand-image {
  width: 144rpx;
}

.brand-image {
  width: 131rpx;
  height: 87rpx;
  margin-top: 35rpx
}

.no-brand-image {
  width: 92rpx;
