/**index.wxss**/
page{
  min-height: 100%;
  background-color: #F2f2f2;
}

view {
  font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
}

.pos-fiexd{
  position: fixed;
  bottom: 0;
  left: 0;
}

.container{
  justify-content:initial;
  background-color: #F2f2f2;
  min-height: 100%;
}
.title-box{
  width: 100%;
  padding-top: 330rpx;
  text-align: center;
  font-size:28rpx;
  color:#999;
  background: url(https://cdn.it120.cc/images/weappshop/icon-cart.png) no-repeat center 205rpx;
  background-size: 100rpx auto;
  margin-bottom: 50rpx;
}
.to-index-btn{
  color:#fff;
  background:#e64340;
  border-radius:6px;
  width:300rpx;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size:28rpx;
}
.list-top{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  box-sizing: border-box;
  display: flex;
  justify-content:space-between;
  padding: 0 30rpx;
  font-size: 28rpx;
  align-items: center;
}
.list-top .label{
  color: #000;
}
.list-top .edit-btn{
  color: #999;
  height: 100%;
}
.goodsList{
  width: 100%;
  /* background-color: #fff; */
  padding-bottom: 100rpx;
}
.a-gooods{
  width: 100%;
  height: 242rpx;
  overflow: hidden;
  background-color: #ffffff;
  margin-bottom: 10rpx;
}
.a-goods-conts{
  display: flex;
  justify-content:space-between;
  padding-left: 110rpx;
  width: 870rpx;
  box-sizing: border-box;
  transition: margin-left 0.2s ease-in-out;
  background: url(data:image/png;base64,) no-repeat 48rpx center;
  background-size: 40rpx auto; 
}
.a-goods-conts.active{
  background: url(data:image/png;base64,) no-repeat 48rpx center;
  background-size: 40rpx auto; 
}
.goods-info{
  /* border-bottom: 1px solid #eee; */
  display: flex;
  /* justify-content:space-between; */
  padding: 24rpx 0 30rpx 0rpx; 
  width: 720rpx;
  box-sizing: border-box;
}
.goods-info .img-box{
  width: 184rpx;
  height:184rpx;
  overflow: hidden;
  margin-right: 24rpx;
  background-color: #d8d8d8;
}
.goods-info .text-box{
  width: 440rpx;
  position: relative;
}
.goods-info .text-box .goods-title{
  font-size: 24rpx;
  color:#333333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  padding-right: 24rpx;
}
.goods-info .text-box .goods-label{
  font-size: 22rpx;
  color:#959595;
  background-color: #f5f5f5;
  height: 64rpx;
  line-height: 30rpx;
  margin-top: 9rpx;
  margin-right: 24rpx;
  margin-bottom: 13rpx;
  padding-left: 9rpx;
  padding-right: 36rpx;
  padding-top: 4rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;

  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 18rpx 10rpx; 
  background-position: right 10rpx center;
}
.goods-info .text-box .goods-price{
  font-size: 26rpx;
  color:#e64340;
}
.goods-info .text-box .buy-num{
  width: 164rpx;
  height: 48rpx;
  line-height: 48rpx;
  position: absolute;
  right: 30rpx;
  bottom: -10rpx;
  display: flex;
  /*justify-content: space-between;*/
  font-size: 24rpx;
  text-align: center
}
.goods-info .text-box .buy-num .jian-btn{
  width: 48rpx;
  height: 100%;
  border-left: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-left-radius: 6rpx;
  border-top-left-radius: 6rpx;
}
.goods-info .text-box .buy-num .jian-btn.disabled{
  background-color: #f5f5f9;
  border-left: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}
.goods-info .text-box .buy-num .jia-btn{
  width: 48rpx;
  height: 100%;
  border-right: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-right-radius: 6rpx;
  border-top-right-radius: 6rpx;
}
.goods-info .text-box .buy-num .jia-btn.disabled{
  background-color: #f5f5f9;
  border-right: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}
.goods-info .text-box .buy-num input{
  width: 68rpx;
  height: 48rpx;
  min-height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  border: 1rpx solid #ccc;
}

.goods-info .img-box .img{
  width: 184rpx;
  height:184rpx;
}

.a-goods-conts .delete-btn{
  width: 120rpx;
  line-height: 220rpx;
  text-align: center;
  background:#e64340;
  font-size: 24rpx;
  color: #fff;
}
.jiesuan-box{
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  border-bottom:1px solid #eee; 
  border-top:1px solid #eee; 
  background-color: #fff;
  z-index: 100;
}
.jiesuan-box .to-pay-btn{
  width:240rpx;
  text-align: center;
  line-height: 100rpx;
  background-color: #e64340;
  font-size:32rpx;
  color:#ffffff;
}
.jiesuan-box .to-pay-btn.no-select{
  background-color: #ccc;
}

.jiesuan-box  .left-price{
  display: flex;
  width: 510rpx;
  justify-content: space-between;
  line-height: 100rpx;
  padding: 0 30rpx 0 0;
  font-size:28rpx;
  box-sizing: border-box;
}
.jiesuan-box .all-selected{
  padding-left: 90rpx;
  color:#000000;
  background: url(https://cdn.it120.cc/images/weappshop/gou.png) no-repeat 30rpx center;
  background-size: 40rpx auto; 
}
.jiesuan-box .all-selected.active{
  background: url(https://cdn.it120.cc/images/weappshop/gou-red.png) no-repeat 30rpx center;
  background-size: 40rpx auto; 
}
.jiesuan-box .total{
  color: #e64340;
}
.show-popup{
  position: fixed;
  top: 0;
  left: 0;
  width:100%;
  height: 100%;
  z-index: 999;
}
.popup-mask{
  position: fixed;
  top: 0;
  left: 0;
  width:100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 5;
}
.popup-contents{
  position: fixed;
  left: 0;
  bottom: 0;
  width:100%;
  height: 1068rpx;
  background-color: #fff;
  z-index: 6;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}
.pop-goods-info{
  display: flex;
  flex-direction: row;
  height: 280rpx;
  border-bottom: 1rpx solid #eee;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
}
.pop-img-box{
  width: 220rpx;
  height: 220rpx;
  overflow: hidden;
  margin-right: 26rpx;
}
.pop-img-box .goods-thumbnail{
  width: 220rpx;
  height: 220rpx;
}
.pop-goods-title{
  width: 484rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size:26rpx;
  color:#000000;
}
.pop-goods-price{
  font-size:30rpx;
  color:#fe403b;
  margin-bottom: 5rpx;
}
.pop-goods-stock{
  font-size: 24rpx;
  color: #999999;
  margin-bottom: 14rpx;
}
.pop-goods-select{
  font-size: 24rpx;
  color: #343434;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.pop-goods-close{
  width: 66rpx;
  height: 66rpx;
  flex: 1;
  background:url(data:image/png;base64,) no-repeat center center;
  background-size: 66rpx auto;
  position: absolute;
  top:0rpx;
  right: 0rpx;
}
.size-label-box{
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.size-label-box  .label{
  font-size:28rpx;
  color:#343434;
  margin-top:35rpx;
}
.size-label-box .label-item-box{
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
}
.size-label-box .label-item{
  font-size:26rpx;
  color:#000;
  background-color: #f5f5f5;
  padding: 14rpx 20rpx;
  border-radius: 14rpx;
  margin: 28rpx 20rpx;
}
.size-label-box .label-item.active{
  background-color: #fff4f4;
  color: #e64340;
  border: 1px solid #e64340;
}

.popup-ok-btn{
  text-align: center;
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  background-color: #ff3938;
  color:#ffffff;
  font-size: 28rpx;
  font-weight: bold;
  bottom: 0rpx;
  position: absolute;
}

.popup-join-btn{
  text-align: center;
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #3e3e3e;
  color:#ffffff;
  font-size: 28rpx;
  font-weight: bold;
  float: left;
  bottom: 0rpx;
}

.popup-buy-btn{  
  text-align: center;
  height: 100rpx;
  width: 50%;
  line-height: 100rpx;
  background-color: #ff3938;
  color:#ffffff;
  font-size: 28rpx;
  font-weight: bold;
  bottom: 0rpx;
}

.popup-good-attr{
  vertical-align: bottom; 
  margin-top: 95rpx
}

.tip-style{
  right: 24rpx;
  width: 14rpx;
  height: 23rpx;
  margin-top:-50rpx;
  position:absolute;
}