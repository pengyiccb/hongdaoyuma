page{
  min-height: 100%;
  background-color: #F2f2f2;
}

view {
    font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
}

.container{
  min-height: 100%;
  /* overflow: hidden;
  overflow-y: hidden; */
}
.sec-wrap{
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 37rpx 36rpx; 
    background-position: left 40rpx center;

    height: 80rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 1rpx;
}

.sec-wrap .shop-title{
    font-size: 30rpx;
    color: #343434;
    line-height: 80rpx;
    margin-left: 90rpx;
}

.sec-wrap .order-status{
    font-size: 26rpx;
    color: #ff3938;
    line-height: 80rpx;
    margin-right: 40rpx;
}

.bottom-fiexd{
  position: fixed;
  bottom: 0;
  left: 0;
}

.order-status .icon-box{
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    margin-right: 30rpx;
}
.order-status .icon-box .icon{
    width: 80rpx;
    height: 80rpx;
}
.order-status .right-text{
    width: 580rpx;
    overflow: hidden;
}
.order-status .right-text .status{
    font-size:28rpx;
    color:#000; 
    margin-bottom: 10rpx;
}
.order-status .right-text .red{
    color:#e64340;
}
.order-status .right-text .des{
    font-size:24rpx;
    color:#999; 
}
.address-sec{
    width: 720rpx;
    margin-left: 30rpx;
    display: flex;
    align-items: center;
    padding: 30rpx 0;
}
.address-sec .icon-box{
    width: 30rpx;
    align-self: flex-start;
    overflow: hidden;
    margin-right: 35rpx;
}
.address-sec .icon-box .icon{
    width: 30rpx;
    height: 30rpx;
}
.address-sec  .right-box{
    width: 620rpx;
}
.address-sec  .right-box .name-tel{
    font-size:28rpx;
    color:#000000;
    margin-bottom: 20rpx;
}
.address-sec  .right-box .text{
    font-size:24rpx;
    color:#888888;
    line-height:36rpx;
    height: 72rpx;
    overflow: hidden;
}
.wuliu-box{
    width: 720rpx;
    margin-left: 30rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    align-items: center;
    padding: 30rpx 0;
}
.wuliu-box .icon-box {
    width: 40rpx;
    height: 40rpx;
    overflow: hidden;
    margin-right: 31rpx;
    align-self: flex-start;
}
.wuliu-box .icon-box .icon{
    width: 40rpx;
    height: 40rpx;
}
.wuliu-box .arrow-right{
    width: 15rpx;
    height: 24rpx;
}
.wuliu-box .arrow-right .arrow{
    width: 15rpx;
    height: 24rpx;
}
.wuliu-box .right-text{
    width: 575rpx;
    margin-right: 30rpx;
}
.wuliu-box .right-text .order-number{
    font-size:28rpx;
    color:#000;
    margin-bottom: 14rpx;
}
.wuliu-box .right-text .wuliu-text,
.wuliu-box .right-text .wuliu-date{
    font-size:24rpx;
    color:#888888;
    line-height:36rpx;
}

.confirm-btn{
    background:#ffffff;
    border:1rpx solid #e64340;
    border-radius:6rpx;
    width:164rpx;
    height:60rpx;
    line-height: 60rpx;
    text-align: center;
    margin: 20rpx 30rpx 20rpx auto;
    font-size:26rpx;
    color:#e64340;
    text-align:center;
}

.goods-time{
    width: 100%;
    height: 284rpx;
}

.goods-time .goods-box{
    width: 100%;
    height: 47rpx;
    background-color: #fff;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    box-sizing: border-box;
    padding-top: 20rpx;
    padding-left: 30rpx;
    padding-bottom: 24rpx;
    font-size: 22rpx;
    line-height: 47rpx;
    color: #828282;
}

.goods-time .goods-box .copy-btn{
    width: 108rpx;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 22rpx;
    color: #525252;
    border-radius: 8rpx;
    text-align: center;
    border: 1rpx solid;
    margin-left: 137rpx;
    margin-bottom: 10rpx;
}

.goods-info{
    width: 100%;
    margin-bottom: 20rpx;
}
.goods-info .row-box{
    width: 100%;
    height: 64rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 24rpx 30rpx 12rpx 30rpx;
    font-size: 30rpx;
    line-height: 64rpx;
    margin-bottom: 1rpx;
    color: #828282;
}
.goods-info .row-box .right-text{
    text-align: right;
}

.goods-info .row-box .right-text.hot{
    color: #fe403b;
}

.goods-info .row-box .row-label{
    color: #333333;
}

.jiesuan-box{
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    border-top:1px solid #eee; 
    background-color: #fff;
    z-index: 4;
}
.jiesuan-box .to-pay-btn{
    width:250rpx;
    text-align: center;
    height: 100%;
    line-height: 100rpx;
    background-color: #e64340;
    font-size:32rpx;
    color:#ffffff;
    border-radius: 0;
}

.jiesuan-box  .left-price{
    display: flex;
    width: 500rpx;
    justify-content:flex-end;
    line-height: 100rpx;
    padding: 0 30rpx 0 0;
    font-size:28rpx;
    box-sizing: border-box;
}

.jiesuan-box .total{
    color: #e64340;
    text-align: right;
}

.goods-list{
    width:100%;
    margin-bottom: 1rpx;
}
.goods-list .list-title{
    font-size: 28rpx;
    color: #000;
    padding: 30rpx 0 25rpx 30rpx;
}
.goods-list  .a-goods{
    width: 100%;
    height: 242rpx;
    margin-bottom: 1rpx;
    display: flex;
    /* padding-top: 24rpx; */
    padding-left: 24rpx;
    padding-right: 24rpx;
    background-color: #fff;
}
.goods-list  .a-goods .img-box{
    width: 184rpx;
    height:184rpx;
    /* overflow: hidden; */
    margin-right: 24rpx;
    margin-top: 24rpx;
    /* background-color: #d8d8d8; */
}

.goods-list .img-box .img{
    width: 184rpx;
    height:184rpx;
}
.goods-list  .a-goods .text-box{
    width: 100%;
    box-sizing: border-box;
    margin-right: 40rpx;
    margin-top: 18rpx;
    position: relative;
}
.a-goods .text-box .goods-arrow{
    position: absolute;
    bottom: 15rpx;
    width: 100%;
 }
.a-goods .text-box .goods-name{
    font-size: 24rpx;
    color:#333333;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    margin-bottom: 18rpx;
 }
.a-goods .text-box .goods-arrow .goods-price{
    font-size: 26rpx;
    color:#e64340;
    float: left;
}
.a-goods .text-box .goods-label{
    font-size: 22rpx;
    color:#959595;
    background-color: #f5f5f5;
    height: 64rpx;
    line-height: 30rpx;
    margin-top: 9rpx;
    margin-right: 24rpx;
    margin-bottom: 13rpx;
    padding-left: 9rpx;
    /* padding-right: 36rpx; */
    padding-top: 4rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.a-goods .text-box .goods-arrow .goods-num{
    font-size: 26rpx;
    color: #999;
    float: right;
}
