/* pages/vipcard/vipcard.wxss */
page{
    min-height: 100%;
    background-color: #F2f2f2;
  }

  view {
    font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
  }

  .container {
    min-height: 100%;
    width: 100%;
  }

  .title-view{
      position: fixed;
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      height: 70rpx;
      background-color: #fff;
      line-height: 70rpx;
      padding-left: 20rpx;
      padding-right: 20rpx;
      left: 0;
      right: 0;
      top: 0;
      z-index: 12;
  }

  .title-view .myvip{
      font-size: 24rpx;
      color: #333333;
  }

  .title-view .sale-detail{
      font-size: 24rpx;
      color: #0096ec;
  }

  .item-list{
      position: absolute;
      top: 70rpx;
      display: flex;
      flex-direction: column;
      width: 100%;
  }

  .card-view{
      position: relative;
      height: 292rpx;
      background-color: #fff;
      margin: 18rpx 12rpx 0rpx 12rpx;
      border-radius: 16rpx;
  }

  .card-view .card-body{
    height: 152rpx;
    /* background: url("data:image/png;base64,") no-repeat center center; */
    /* background-color: #0096ec; */
    background-size: 206rpx 157rpx;
    border-radius: 16rpx;
  }

  .card-view .card-body.unused{
    background: url("data:image/png;base64,") no-repeat center center;
  }


  .card-view .card-body .card-info{
      width: 80%;
      height: 90rpx;
      position: absolute;
      top: 46rpx;
      left: 52rpx;
  }

  .card-info .card-name{
      color: #fff;
      font-size: 40rpx;
  }

  .card-info .card-time{
      color: #fff;
      font-size: 18rpx;
  }

  .card-view .card-body .card-btn{
      position: absolute;
      top: 44rpx;
      right: 44rpx;
      width: 51rpx;
      height: 51rpx;
      background: url("data:image/png;base64,") no-repeat center center;
      background-size: 51rpx 51rpx
  }

  .card-view .line-dot{
      position: absolute;
      width: 100%;
      height: 8rpx;
      background: url("data:image/png;base64,") repeat center;
      background-size: auto 8rpx;
      top:144rpx;
  }

  ::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
    }

  .card-view .card-foot{
      height: 135rpx;
      position: relative;
  }

  .card-foot .use-item{
      position: absolute;      
      height: 25rpx;
      width: 450rpx;
      background: url("data:image/png;base64,") no-repeat;
      background-size: 22rpx 22rpx;
      background-position: left center;
      font-size: 22rpx;
      color: #a3a3a3;
      line-height: 25rpx;
      padding-left: 40rpx;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
  }

  .card-foot .use-item.first{
    top: 26rpx;
    left: 60rpx;
  }

  .card-foot .use-item.second{
    top: 100rpx;
    left: 60rpx;
  }

  .card-foot .line-dot{
      top: 54rpx;
      left: 67rpx;
      background: url("data:image/png;base64,") no-repeat center;
      background-size: 4rpx 42rpx;
      width: 4rpx;
      height: 42rpx;
  }
  
  .card-foot .use-tip{
      font-size: 25rpx;
      color: #ff0000;
      width: 110rpx;
      height: 50rpx;
      line-height: 50rpx;
      border-radius: 26rpx;
      position: absolute;
      border: 1rpx solid #eb5232;
      right: 35rpx;
      top: 50rpx;
      text-align: center;
  }