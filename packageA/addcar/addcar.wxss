.body-view {
    width: 100%;
    height: 100%;
    font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif
}

.body-head {
    width: 100%;
    height: 78rpx;
    background: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 30rpx;
    color: rgb(51, 51, 51)
}

.head-brand-image {
    width: 84rpx;
    height: 56rpx;
}

.head-brand-text {
    margin-left: 24rpx;
    color: rgb(254, 64, 59);
    border-bottom: 4rpx rgb(254, 64, 59) solid;
    white-space: pre-wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    max-width: 200rpx;
}

.head-brand-text.select {
    margin-left: 0;
    color: rgb(51, 51, 51);
    border-bottom: 0
}

.head-series-text{
    margin-left: 24rpx;
    color: rgb(254, 64, 59);
    border-bottom: 4rpx rgb(254, 64, 59) solid;
    /*line-height: 78rpx;*/
    white-space: pre-wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    max-width: 200rpx
}

.head-series-text.select{
    color: rgb(51, 51, 51);
    border-bottom: 0;
    /*line-height: 78rpx*/
}

.head-time-text{
    margin-left: 24rpx;
    color: rgb(254, 64, 59);
    border-bottom: 4rpx rgb(254, 64, 59) solid;
    white-space: pre-wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical
}

.head-time-text.select{
    color: rgb(51, 51, 51);
    border-bottom: 0
}

.head-version-text{
    margin-left: 24rpx;
    color: rgb(254, 64, 59);
    border-bottom: 4rpx rgb(254, 64, 59) solid;
    white-space: pre-wrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical
