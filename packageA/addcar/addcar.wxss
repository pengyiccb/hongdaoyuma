.brand-hot-info{
    margin-top: 40rpx;
    background: #fff;
    /*display: flex;
    flex-direction: column*/
}

.brand-info {
    width: 100%
}

.brand-wordindex {
    height: 50rpx;
    font-size: 24rpx;
    padding-left: 30rpx;
    color: rgb(51, 51, 51);
    line-height: 50rpx;
    font-weight: bold
}

.brand-block{
    background: #fff;
}
.brand-line {
    height: 120rpx;
    display: flex;
    flex-direction: row;
    line-height: 120rpx;
    align-items: center;
    font-size: 32rpx;
    color: rgb(110, 110, 110)
}


.brand-line-image {
    height: 56rpx;
    width: 84rpx;
}

.brand-hot-name{
    height: 80rpx;
    border-bottom:1rpx #e5e5e5 solid; 
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 30rpx;
    font-weight: bold;
    padding-left: 30rpx;
}

.brand-hot-name-flag{
    background: rgb(254, 64, 59);
    height: 28rpx;
    width: 6rpx
}

.brand-hot-name-text{
    margin-left: 12rpx;
}

.brand-hot-body {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    /*position: relative;
    float: left;*/
    background: #fff
}

.brand-hot-one{
    width: 150rpx;
    height: 150rpx;
    color: rgb(102, 102, 102);
    display: flex;
    flex-direction: column;
    align-items: center
}

.hot-brand-image{
    height: 77rpx;
