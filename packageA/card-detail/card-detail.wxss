/* pages/vipcard/vipcard.wxss */
page{
    min-height: 100%;
    background-color: #F2f2f2;
  }

  view {
    font-family: "PingFang SC", "Helvetica Neue", Helvetica, "Nimbus Sans L", Arial, "Liberation Sans", "Hiragino Sans GB", "Source Han Sans CN Normal", "Microsoft YaHei", "Wenquanyi Micro Hei", "WenQuanYi Zen Hei", "ST Heiti", SimHei, "WenQuanYi Zen Hei Sharp", sans-serif;
  }

  .container {
    min-height: 100%;
    width: 100%;
  }

  .card-view{
      position: absolute;
      /* height: 1160rpx; */
      background-color: #fff;
      top: 24rpx;
      /* bottom: 306rpx; */
      left: 12rpx;
      right: 12rpx;
      border-radius: 16rpx;
  }

  .card-view .card-body{
    height: 152rpx;
    /* background-color: #0096ec; */
    border-radius: 16rpx;
  }

  .card-view .card-body .card-info{
      width: 80%;
      height: 90rpx;
      position: absolute;
      top: 46rpx;
      left: 52rpx;
  }

  .card-info .card-name{
      color: #fff;
      font-size: 40rpx;
  }

  .card-info .card-time{
      color: #fff;
      font-size: 18rpx;
  }

  ::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
    }

  .card-view .card-body .card-btn{
      position: absolute;
      top: 46rpx;
      right: 42rpx;
      width: 171rpx;
      height: 52rpx;
      background: url("data:image/png;base64,") no-repeat center center;
      background-size: 171rpx 52rpx
  }

  .card-view .line-dot{
      position: absolute;
      width: 100%;
      height: 8rpx;
      background: url("data:image/png;base64,") repeat center;
      background-size: auto 8rpx;
      top:144rpx;
  }

  .card-view .service-title{
      height: 56rpx;
      line-height: 56rpx;
      background-color: #f8f8f8;
      border-radius: 8rpx;
      color: #343434;
      font-size: 30rpx;
      margin-left: 18rpx;
      margin-top: 40rpx;
      margin-right: 18rpx;
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      padding-left: 34rpx;
      padding-right: 32rpx;
  }

  .card-view .service-list{
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    top: 260rpx;
    bottom: 234rpx;
    width: 616rpx;
    left: 50rpx;
  }

  .service-list .service-item{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 70rpx;
    line-height: 70rpx;
    background: url(data:image/png;base64,) no-repeat left center;
    background-size: 40rpx auto; 
    font-size: 24rpx;
    color: #585858;
    border-bottom: 1rpx dashed #eaeaea;
    padding-left: 52rpx;
  }

  .service-list .service-item.active{
    background: url(data:image/png;base64,) no-repeat left center;
    background-size: 40rpx auto; 
    color: #fe7572;
  }

  .service-list .service-item.used{
    color: #c3c3c3;
  }
  
  .service-list .service-item .service-name{
    width: 500rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
  }

  .card-view .car-number{
      position: absolute;
      display: flex;
      flex-direction: row;
      align-items: center;
      height: 90rpx;
      line-height: 90rpx;
      color: #343434;
      font-size: 30rpx;
      border-radius: 16rpx;
      border: 2rpx solid #d0d0d0;
      left: 50rpx;
      right: 50rpx;
      bottom: 124rpx;
  }

  .car-number .car-city{
      background: url("data:image/png;base64,") no-repeat;
      background-size: 18rpx 10rpx;
      background-position: 90% center;
      height: 100%;
      width: 100rpx;
      text-align: center;
  }

  .car-number .car-line{
    height: 51rpx;
    width: 1rpx;
    border-left: 1rpx solid #e5e5e5;
  }

  .car-number .car-input{
      height: 100%;
      flex: 1;
      padding-left: 35rpx;
      font-size: 30rpx;
      color: #343434;
      position: relative;
  }

  .car-number .car-placeholder{
      font-size: 30rpx;
      color: #d0d0d0;
  }

  .card-view .yuyue-btn{
      position: absolute;
      color: #fff;
      background-color: #f12c20;
      border-radius: 16rpx;
      font-size: 37rpx;
      height: 90rpx;
      line-height: 90rpx;
      text-align: center;
      bottom: 14rpx;
      left: 50rpx;
      right: 50rpx;
  }

  .card-view .yuyue-btn.no-select{
    background-color: #ccc;
  }
  
  .empty-box{
      border: 1rpx solid blue;
      height: 100rpx;
      position: absolute;
      left: 2rpx;
      right: 2rpx;
  }

  /* popupCardInfo start */
.popupMask{
    background-color:rgba(0, 0, 0, 0.5);
    width:100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 2;
}
.popupBg {
    background: #ffffff;
    width: 672rpx;
    height: 980rpx;
    border-radius: 20rpx;
    position: relative;
    left: 39rpx;
    top: 113rpx;
    z-index: 3;
}
.pop-goods-close{
    width: 66rpx;
    height: 66rpx;
    flex: 1;
    background:url(data:image/png;base64,) no-repeat center center;
    background-size: 66rpx auto;
    position: absolute;
    top:0rpx;
    right: 0rpx;
}
.textTitle{
    color:rgb(38,38,38);
    font-size: 36rpx;
    line-height: 36rpx;
    position: relative;
    top: 40rpx;
    text-align:center;
}
.textTitlePS{
    color:rgb(38,38,38);
    font-size: 36rpx;
    line-height: 36rpx;
    position: relative;
    left: 50rpx; 
    top: 110rpx;
}
.scrollViewFrame{
    width: 672rpx;
    height: 700rpx;
    position: relative;
    top: 150rpx;
}
.textInfo{
    width: 572rpx;
    position: relative;
    left: 50rpx; 
    top: 10rpx;
}
.textSubTitle{
    color:rgb(51,51,51);
    font-size: 28rpx;
    line-height: 28rpx;
    margin-bottom: 22rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.textSubInfo{
    color:rgb(155,155,155);
    font-size: 22rpx;
    line-height: 22rpx;
    position: relative;
    margin-bottom: 26rpx;
}
.separateDashed{
    width: 612rpx;
    border: 1rpx solid #f2f2f2;
    border-style: dashed;
    position: relative;
    left: -20rpx;
    margin-bottom: 26rpx;
}
  /* popupCardInfo end */